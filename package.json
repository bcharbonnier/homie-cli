  "bugs": {
    "url": "https://github.com/bcharbonnier/homie-sentinel-server/issues"
  },
  "homepage": "https://github.com/bcharbonnier/homie-sentinel-server#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "bonjour": "^3.5.0",
    "chalk": "^2.3.0",
    "chokidar": "^1.7.0",
    "commander": "^2.12.1",
    "express": "^4.16.2",
    "filesize": "^3.5.11",
    "glob": "^7.1.2",
    "mqtt": "^2.14.0",
    "multer": "^1.3.0",
    "prompt": "^1.0.0",
    "unzip": "^0.1.11",
    "ws": "^3.3.2"
