{
  "name": "homie-cli",
  "version": "1.0.0",
  "description":
    "Homie Server CLI create a server with an interactive UI to manage local instances of Homie devices",
  "main": "index.js",
  "bin": {
    "homie": "bin/index.js"
  },
  "scripts": {
    "console:start": "cd console && npm run start",
    "console:build": "cd console && npm run build",
    "console:prod": "NODE_ENV=production && npm run console:build && homie server start",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcharbonnier/homie-sentinel-server.git"
  },
